{
  "name": "kriss/yii2-calendar-schedule",
  "description": "Yii2 Calendar Schedule",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "fullcalendar",
    "calendar",
    "schedule"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "kriss",
      "email": "462679766@qq.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-jui": "^2.0",
    "npm-asset/fullcalendar": "3.9.0"
  },
  "require-dev": {
    "kriss/foxy-yii2-convert": "v1.4"
  },
  "provide": {
    "bower-asset/jquery": "*",
    "bower-asset/bootstrap": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/jquery.inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*",
    "bower-asset/jquery-ui": "*",
    "npm-asset/fullcalendar": "*"
  },
  "config": {
    "foxy-yii2-convert": {
      "target-map": {
        "bower-asset/jquery-ui": "npm-asset/components-jqueryui"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "kriss\\calendarSchedule\\": "src"
    }
  }
}
